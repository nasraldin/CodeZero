<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>CodeZero.Shared.Constants</AssemblyName>
    <PackageId>CodeZero.Shared.Constants</PackageId>
    <!--Change this {Version} if you fix a bug or add a new rule, for shipping new package.-->
    <PackageVersion>6.0.1</PackageVersion>
    <Version>6.0.1</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>A set of common constants shared for CodeZero Framework</Description>
    <PackageDescription>A set of common constants shared for CodeZero Framework</PackageDescription>
    <PackageTags>.net core;boilerplate;modular;best-practices</PackageTags>
    <!--Change this {PackageReleaseNotes}, add your relase notes for fix a bug or a new rule.-->
    <PackageReleaseNotes>Update constants</PackageReleaseNotes>
    <Platforms>AnyCPU;ARM64</Platforms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.Shared.Constants.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ARM64'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.Shared.Constants.xml</DocumentationFile>
  </PropertyGroup>

  <!--Import common packages are used across all projects-->
  <Import Project="..\..\Common.props" />

</Project>