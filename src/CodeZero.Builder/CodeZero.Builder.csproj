<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>CodeZero.Builder</AssemblyName>
    <PackageId>CodeZero.Builder</PackageId>
    <!--Change this {Version} if you fix a bug or add a new rule, for shipping new package.-->
    <PackageVersion>6.0.1</PackageVersion>
    <Version>6.0.1</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Initializes a new instance of the WebApplicationBuilder class with preconfigured defaults</Description>
    <PackageDescription>Initializes a new instance of the WebApplicationBuilder class with preconfigured defaults</PackageDescription>
    <PackageTags>.net core;boilerplate;clean architecture;best-practices</PackageTags>
    <!--Change this {PackageReleaseNotes}, add your relase notes for fix a bug or a new rule.-->
    <PackageReleaseNotes>CodeZero.Builder .net 6 Inial release</PackageReleaseNotes>
    <Platforms>AnyCPU;ARM64</Platforms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.Builder.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|ARM64'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.Builder.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\CodeZero.ServiceInstaller\CodeZero.ServiceInstaller.csproj" />
  </ItemGroup>

  <!--Import common packages are used across all projects-->
  <Import Project="..\..\Common.props" />

</Project>