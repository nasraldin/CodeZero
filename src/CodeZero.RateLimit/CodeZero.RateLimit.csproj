<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>CodeZero.RateLimit</AssemblyName>
    <PackageId>CodeZero.RateLimit</PackageId>
    <!--Change this {Version} if you fix a bug or add a new rule, for shipping new package.-->
    <PackageVersion>6.0.1</PackageVersion>
    <Version>6.0.1</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>Add RateLimit to your application</Description>
    <PackageDescription>Add RateLimit to your application</PackageDescription>
    <PackageTags>.net core;rate limit;best-practices</PackageTags>
    <!--Change this {PackageReleaseNotes}, add your relase notes for fix a bug or a new rule.-->
    <PackageReleaseNotes>CodeZero.RateLimit .net 6 Inial release</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.RateLimit.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AspNetCoreRateLimit" Version="4.0.2" />
    <PackageReference Include="AspNetCoreRateLimit.Redis" Version="1.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CodeZero.Configuration\CodeZero.Configuration.csproj" />
  </ItemGroup>

  <!--Import common packages are used across all projects-->
  <Import Project="..\..\Common.props" />

</Project>
