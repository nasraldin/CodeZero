<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <AssemblyName>CodeZero.ServiceInstaller</AssemblyName>
    <PackageId>CodeZero.ServiceInstaller</PackageId>
    <!--Change this {Version} if you fix a bug or add a new rule, for shipping new package.-->
    <PackageVersion>6.0.0</PackageVersion>
    <Version>6.0.0</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>CodeZero.ServiceInstaller enable dependency injection (DI)</Description>
    <PackageDescription>CodeZero.ServiceInstaller enable dependency injection (DI)</PackageDescription>
    <PackageTags>.net core;clean architecture;logging;serilog;best-practices</PackageTags>
    <!--Change this {PackageReleaseNotes}, add your relase notes for fix a bug or a new rule.-->
    <PackageReleaseNotes>CodeZero.ServiceInstaller .net 6 Inial release</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>obj\Debug\net6.0\CodeZero.ServiceInstaller.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.6" />
    <PackageReference Include="Microsoft.AspNetCore.DataProtection.StackExchangeRedis" Version="6.0.6" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CodeZero.Antiforgery\CodeZero.Antiforgery.csproj" />
    <ProjectReference Include="..\CodeZero.Exceptional\CodeZero.Exceptional.csproj" />
    <ProjectReference Include="..\CodeZero.HealthChecks\CodeZero.HealthChecks.csproj" />
    <ProjectReference Include="..\CodeZero.Localization\CodeZero.Localization.csproj" />
    <ProjectReference Include="..\CodeZero.MiniProfiler\CodeZero.MiniProfiler.csproj" />
    <ProjectReference Include="..\CodeZero.RateLimit\CodeZero.RateLimit.csproj" />
    <ProjectReference Include="..\CodeZero.ReverseProxy\CodeZero.ReverseProxy.csproj" />
    <ProjectReference Include="..\CodeZero.Serilog\CodeZero.Serilog.csproj" />
    <ProjectReference Include="..\CodeZero.Shared.Exception\CodeZero.Shared.Exception.csproj" />
    <ProjectReference Include="..\CodeZero.Swagger\CodeZero.Swagger.csproj" />
  </ItemGroup>

  <!--Import common packages are used across all projects-->
  <Import Project="..\..\Common.props" />

</Project>
